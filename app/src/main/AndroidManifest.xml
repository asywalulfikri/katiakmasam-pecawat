<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asywalul.pesawat.perang.android">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_pesawat"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitle">
        <activity android:name="aywalul.pesawat.perang.android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="aywalul.pesawat.perang.android.GameActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name="aywalul.pesawat.perang.android.InterestialLogout"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>